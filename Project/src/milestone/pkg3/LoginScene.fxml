<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="milestone.pkg3.LoginSceneController">
   <children>
      <VBox prefHeight="400.0" prefWidth="106.0" style="-fx-background-color: blue;" />
      <VBox layoutX="492.0" prefHeight="400.0" prefWidth="116.0" style="-fx-background-color: blue;" />
      <Label layoutX="123.0" layoutY="149.0" style="-fx-font-style: italic;" text="UserName">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Label layoutX="129.0" layoutY="215.0" style="-fx-font-style: italic;" text="Password">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTxt" layoutX="255.0" layoutY="145.0" onMouseClicked="#clearUNOnMouseClick" prefHeight="40.0" prefWidth="182.0">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
      <TextField fx:id="passwordTxt" layoutX="255.0" layoutY="214.0" onMouseClicked="#clearPWOnMouseClick" prefHeight="40.0" prefWidth="182.0">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
      <Button fx:id="LoginButton" layoutX="125.0" layoutY="305.0" mnemonicParsing="false" onAction="#loginOnButtonClick" prefHeight="25.0" prefWidth="312.0" text="Log in" />
      <ComboBox fx:id="userComboBox" layoutX="287.0" layoutY="79.0" onAction="#selectuserOnButtonClick" prefWidth="150.0" />
      <Label layoutX="123.0" layoutY="83.0" prefHeight="17.0" prefWidth="86.0" text="Selected use:" />
      <Label fx:id="selecteduserLabel" layoutX="228.0" layoutY="83.0" text="Label" />
      <Label layoutX="280.0" layoutY="272.0" onMouseClicked="#changePWOnMouseClick" prefHeight="17.0" prefWidth="157.0" text="Forgot password?Click Here!" />
   </children>
</AnchorPane>
