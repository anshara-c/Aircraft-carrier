<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="milestone.pkg3.MissionReportController">
   <children>
      <Label layoutX="19.0" layoutY="43.0" text="Description of the task carried out of the given mission:" />
      <Label layoutX="26.0" layoutY="218.0" text="Pilot name:" />
      <Label layoutX="26.0" layoutY="192.0" text="Mission index" />
      <TextArea fx:id="mReportArea" layoutX="17.0" layoutY="70.0" prefHeight="106.0" prefWidth="525.0" />
      <TextField fx:id="missionIndex" layoutX="117.0" layoutY="188.0" />
      <TextField fx:id="pilotName" layoutX="117.0" layoutY="214.0" />
      <TextArea fx:id="complainReportArea" layoutX="17.0" layoutY="281.0" prefHeight="65.0" prefWidth="432.0" />
      <Label layoutX="13.0" layoutY="251.0" text="WRITE DOWN ANY COMPLAIN ON ANY SQUADRON MEMBER FOR THEM TO BE HELD UNDER OBSERVATION:" />
      <Button fx:id="saveReport" layoutX="474.0" layoutY="286.0" mnemonicParsing="false" onAction="#saveReportOnButtonClick" prefHeight="57.0" prefWidth="106.0" text="Save report" />
      <Button fx:id="go2Dashboard" layoutX="474.0" layoutY="353.0" mnemonicParsing="false" onAction="#go2DashboardOnButtonClick" text="Go to dashboard" />
